<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:vm="clr-namespace:Romanesco.Host.ViewModels"
                    x:Class="Romanesco.Host.Views.EditorTemplates">
    <DataTemplate x:DataType="{x:Type vm:LoadedProjectViewModel}" x:Key="LoadedProject">
        <Label Text="プロジェクトが読み込まれています。"/>
    </DataTemplate>

    <DataTemplate x:DataType="{x:Type vm:NullProjectViewModel}" x:Key="NullProject">
        <Label Text="プロジェクトが読み込まれていません。"/>
    </DataTemplate>
</ResourceDictionary>