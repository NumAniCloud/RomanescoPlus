<Window x:Class="Romanesco.Host2.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Romanesco.Host2"
        xmlns:vm="clr-namespace:Romanesco.Host2.ViewModels"
        xmlns:v="clr-namespace:Romanesco.Host2.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <vm:EditorViewModel/>
    </Window.DataContext>

    <Grid>
        <ContentControl Content="{Binding Project.Value}">
            <ContentControl.ContentTemplateSelector>
                <v:DataModelTemplateSelector x:Name="TemplateSelector"/>
            </ContentControl.ContentTemplateSelector>
        </ContentControl>
    </Grid>
</Window>
